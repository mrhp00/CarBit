{
  "expo": {
    "name": "Carbit",
    "slug": "carbit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/app-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.rayancode.carbit",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/app-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {},
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-font",
      "expo-localization"
    ],
    "scheme": "carbit",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d8d9a23b-a3ca-4e10-b8a2-b107163eec64"
      }
    }
  }
}
