{
  "expo": {
    "name": "Carbit",
    "slug": "carbit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {},
    "web": {},
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-font",
      "expo-localization"
    ],
    "scheme": "carbit"
  }
}
